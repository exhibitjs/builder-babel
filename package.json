{
  "name": "exhibit-builder-babel",
  "description": "Exhibit builder for transforming JavaScript files using Babel",
  "version": "0.2.0",
  "author": "Callum Locke",
  "dependencies": {
    "babel-core": "^5.8.25"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "nodemon": "^1.7.1",
    "rimraf": "^2.4.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "exhibit": "^0.7.0"
  },
  "repository": "exhibitjs/builder-babel",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "buildwatch": "rimraf dist && babel src --out-dir dist --watch",
    "prepublish": "npm run build"
  }
}
