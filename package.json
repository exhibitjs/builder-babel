{
  "name": "exhibit-builder-babel",
  "description": "Exhibit builder for transforming JavaScript files using Babel",
  "version": "0.0.1",
  "author": "Callum Locke",
  "bugs": {
    "url": "https://github.com/exhibitjs/exhibit-builder-babel/issues"
  },
  "dependencies": {
    "babel-core": "^5.8.25",
    "convert-source-map": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "nodemon": "^1.7.1"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/exhibitjs/exhibit-builder-babel",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/exhibitjs/exhibit-builder-babel.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "watch": "nodemon --exec 'npm run build' --watch src --watch index.js --watch package.json"
  }
}
